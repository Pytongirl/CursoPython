def division(var1,var2):
    resultado=var1/var2
    print(f"Tu resultado es {resultado}")
    return resultado

def variables():
    valor1 = int(input("Ingresa tu primer valor:\n"))
    valor2 = int(input("Ingrese tu segundo valor:\n"))
    return valor1,valor2


paso = variables()

division(paso[0],paso[1])
